QScrollBar:vertical {
     background: transparent;
     background-image: url("Videowindow/newslider.png");
     border-style: outset;
     border-width: 0px;
     border-radius: 0px;
     border-color: transparent;
     width: 20px;
     height: 290px;
     margin: 22px 0 25px 0;
 }

 QScrollBar::handle:vertical {
     background: transparent;
     background-image: url("Videowindow/sliderbutton.png");
     border-width: 0px;
     border-radius: 0px;
     border-color: transparent;
     padding: 18;
 }

 QScrollBar::add-line:vertical {
     background-image: url("Videowindow/minus.png");
     height: 32px;
     width: 20px;
     border-style: outset;
     border-width: 0px;
     border-radius: 0px;
     border-color: transparent;
     subcontrol-position: bottom;
     subcontrol-origin: margin;
 }

 QScrollBar::sub-line:vertical {
     background-image: url("Videowindow/plus.png");
     height: 32px;
     width: 20px;
     border-style: outset;
     border-width: 0px;
     border-radius: 0px;
     border-color: transparent;
     subcontrol-position: top;
     subcontrol-origin: margin;
 }

 QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
     background: transparent;
 }

 QLabel [accessibleName="Label"]{
    background-image: url("Videowindow/label.png");
    border-style: outset;
    border-width: 2px;
    border-radius: 5px;
    border-color: transparent;
    font-family: "Grand Hotel";
    font-size: 50px;
    padding: 0;
    margin: 0;
    width: 592px;
    height: 72px;
}